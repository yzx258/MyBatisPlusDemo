<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mp.mapper.RatioMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.example.mp.entity.Ratio">
		<id column="id" property="id" />
		<result column="create_time" property="createTime" />
		<result column="update_time" property="updateTime" />
		<result column="version" property="version" />
		<result column="deleted" property="deleted" />
		<result column="type" property="type" />
		<result column="status" property="status" />
		<result column="start_bet_price" property="startBetPrice" />
		<result column="bet_ratio" property="betRatio" />
		<result column="bet_range" property="betRange" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, create_time AS createTime, update_time AS updateTime, version, deleted, type, status, start_bet_price AS startBetPrice, bet_ratio AS betRatio, bet_range AS betRange
    </sql>

</mapper>
